generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum OrderStatus {
  PENDING_CONFIRMATION
  PENDING_PICKUP
  PENDING_DELIVER
  DELIVERED
  RETURNED
  CANCELLED
}

enum VerificationType {
  REGISTER
  FORGOT_PASSWORD
}

enum UserStatus {
  ACTIVE
  BLOCKED
}

enum HttpMethod {
  GET
  POST
  PUT
  PATCH
  DELETE
  HEAD
  OPTIONS
}

enum RoleEnum {
  ADMIN
  CLIENT
  SELLER
}
